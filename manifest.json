{
  "manifest_version": 2,
  "name": "Помощник для сайта detmir.ru",
  "description": "Автоматически заполняет имя и телефон при оформлении заказа",
  "version": "1.2",
  "content_scripts": [
    {
      "matches": ["https://www.detmir.ru/cart/"],
      "js": ["libs/toastify-js.js", "emulator.js", "toast.js", "basket-helper.js"],
      "css": ["libs/toastify.min.css"]
    }
  ],
  "icons": {
    "32": "images/D32.png",
    "64": "images/D64.png",
    "128": "images/D128.png"
  },
  "permissions":[
    "storage",
    "https://www.detmir.ru/cart/"
  ],
  "options_page": "options.html"
}
